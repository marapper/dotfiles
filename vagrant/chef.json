{
    "app": {
        "server": "vr.vagrant",
        "recipe": "symfony_lamp",
        "guest_ip": "127.0.0.1",
        "ip": "127.0.0.1",
        "web_apps": {
            "site-ru": {
                "host_project_folder": "site-ru/web",
                "guest_docroot": "site-ru/web",
                "server_name": "site.vr.vagrant",
                "server_port": 8010,
                "php_timezone": "Europe/Moscow"
            },
            "pamm6": {
                "host_project_folder": "pamm6/web",
                "guest_docroot": "pamm6/web",
                "server_name": "pamm6.vr.vagrant",
                "server_port": 8015,
                "php_timezone": "Europe/Moscow"
            }
        },
        "php_timezone_cli": "Europe/Moscow"
    },
    "xdebug": {
      "remote_enable": 1,
      "remote_host"  : "127.0.0.1",
      "remote_port"  : "9002"
    },
    "postgresql": {
      "listen_addresses": "localhost",
      "port": 5432,
      "max_connections": 100,
      "users": [
        {
          "username": "test",
          "password": "test_vras",
          "superuser": true,
          "createdb": true,
          "login": true
        }
      ],
      "databases": [
        {
            "name": "vras",
            "owner": "test",
            "encoding": "UTF8",
            "locale": "ru_RU.UTF-8"
            /*, "postgis": true*/
        }
      ],
      "locale": "ru_RU.UTF-8",
      "lc_messages": "ru_RU.UTF-8",
      "lc_monetary": "ru_RU.UTF-8",
      "lc_numeric": "ru_RU.UTF-8",
      "lc_time": "ru_RU.UTF-8",
      "initdb_options": "--locale=ru_RU.UTF-8",
      "default_text_search_config": "pg_catalog.russian"
    },
    "postgis": {
        "version": "1.5"
    },
    "run_list": [
        "recipe[postgresql::server]"
    ]
}